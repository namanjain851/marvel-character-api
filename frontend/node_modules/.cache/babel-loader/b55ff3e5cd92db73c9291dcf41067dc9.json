{"ast":null,"code":"import MarvelCharacter from \"./components/MarvelCharacter.vue\";\n\nconst axios = require('axios');\n\naxios.defaults.baseURL = \"http://localhost:8080\";\nexport default {\n  name: \"App\",\n\n  data() {\n    return {\n      inputText: null,\n      loading: false,\n      pageNo: 1,\n      response: null,\n      next_total: 0\n    };\n  },\n\n  components: {\n    MarvelCharacter\n  },\n  methods: {\n    getData: function () {\n      var self = this;\n      this.loading = true;\n      axios.get('/api/' + this.inputText, {\n        params: {\n          page: this.pageNo\n        }\n      }).then(function (response) {\n        self.response = response.data.data.results;\n        self.loading = false;\n        self.next_total = response.data.data.total - self.pageNo * 10;\n      });\n    },\n\n    prevPage() {\n      this.pageNo = this.pageNo - 1;\n      this.getData();\n    },\n\n    nextPage() {\n      this.pageNo = this.pageNo + 1;\n      this.getData();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAaA,OAAOA,eAAP,MAA4B,kCAA5B;;AACA,MAAMC,KAAI,GAAIC,OAAO,CAAC,OAAD,CAArB;;AACAD,KAAK,CAACE,QAAN,CAAeC,OAAf,GAAyB,uBAAzB;AACA,eAAe;EACbC,IAAI,EAAE,KADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,SAAS,EAAE,IADN;MAELC,OAAO,EAAE,KAFJ;MAGLC,MAAM,EAAE,CAHH;MAILC,QAAQ,EAAE,IAJL;MAKLC,UAAU,EAAE;IALP,CAAP;EAOD,CAVY;;EAWbC,UAAU,EAAE;IACVZ;EADU,CAXC;EAcba,OAAO,EAAC;IACNC,OAAM,EAAG,YAAU;MACjB,IAAIC,IAAG,GAAI,IAAX;MACA,KAAKP,OAAL,GAAe,IAAf;MACAP,KAAK,CAACe,GAAN,CAAU,UAAQ,KAAKT,SAAvB,EAAiC;QAC/BU,MAAM,EAAC;UACLC,IAAI,EAAC,KAAKT;QADL;MADwB,CAAjC,EAIGU,IAJH,CAIQ,UAAST,QAAT,EAAkB;QACtBK,IAAI,CAACL,QAAL,GAAgBA,QAAQ,CAACJ,IAAT,CAAcA,IAAd,CAAmBc,OAAnC;QACAL,IAAI,CAACP,OAAL,GAAe,KAAf;QACAO,IAAI,CAACJ,UAAL,GAAkBD,QAAQ,CAACJ,IAAT,CAAcA,IAAd,CAAmBe,KAAnB,GAA4BN,IAAI,CAACN,MAAL,GAAY,EAA1D;MACH,CARD;IASD,CAbK;;IAcNa,QAAQ,GAAE;MACR,KAAKb,MAAL,GAAe,KAAKA,MAAL,GAAc,CAA7B;MACA,KAAKK,OAAL;IACD,CAjBK;;IAkBNS,QAAQ,GAAE;MACR,KAAKd,MAAL,GAAc,KAAKA,MAAL,GAAc,CAA5B;MACA,KAAKK,OAAL;IACF;;EArBM;AAdK,CAAf","names":["MarvelCharacter","axios","require","defaults","baseURL","name","data","inputText","loading","pageNo","response","next_total","components","methods","getData","self","get","params","page","then","results","total","prevPage","nextPage"],"sourceRoot":"","sources":["/media/naman/1eeaa88a-be5a-47ab-8656-55050f02a407/Mass (2021) [1080p] [WEBRip] [5.1] [YTS.MX]/demo/frontend/src/App.vue"],"sourcesContent":["<template>\n  <h1>Hello KOLO</h1>\n  <input type=\"text\" v-model=\"inputText\"/>\n  <button v-on:click=\"getData\">Submit</button>\n  <p v-show = \"loading\"> Loading...</p>\n  <button v-on:click=\"prevPage\" :disabled = \"pageNo === 1\">Prev Page</button>\n  <button v-on:click=\"nextPage\" :disabled = \"next_total <= 0\">Next Page</button>\n  <MarvelCharacter \n    :characterInfo = \"response\"\n  />\n</template>\n\n<script>\nimport MarvelCharacter from \"./components/MarvelCharacter.vue\";\nconst axios = require('axios');\naxios.defaults.baseURL = \"http://localhost:8080\";\nexport default {\n  name: \"App\",\n  data() {\n    return {\n      inputText: null,\n      loading: false,\n      pageNo: 1,\n      response: null,\n      next_total: 0,\n    };\n  },\n  components: {\n    MarvelCharacter,\n  },  \n  methods:{\n    getData :function(){\n      var self = this;\n      this.loading = true;\n      axios.get('/api/'+this.inputText,{\n        params:{\n          page:this.pageNo\n        }\n      }).then(function(response){\n          self.response = response.data.data.results;\n          self.loading = false;\n          self.next_total = response.data.data.total - (self.pageNo*10);\n      })\n    },\n    prevPage(){\n      this.pageNo  = this.pageNo - 1;\n      this.getData();\n    },\n    nextPage(){\n      this.pageNo = this.pageNo + 1;\n      this.getData();\n    }\n  },\n};\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-